#!/command/with-contenv bashio
# shellcheck shell=bash
# ==============================================================================
# Init postfix - generate config from options
# ==============================================================================

set -e

bashio::log.info "Starting clickhouse-server..."
exec /usr/bin/clickhouse-server --config-file=/data/config/clickhouse-server/config.xml --log-file=/dev/stdout --errorlog-file=/dev/stderr --daemon
bashio::log.info "clickhouse-server started!"
